@model List<WebApplication2.Models.Coffee>

<link href="~/css/CoffeeStyle.css" rel="stylesheet" type="text/css" />


@{
    ViewBag.Title = "title";
    Layout = "_CoffeeLayout";
}

<div>
    <h1>
        Analytics and Backend viewing
    </h1>
    <p>
        Welcome to the backend analytical page. This page is supposed to showcase what is going on this website as it navigates
        through the different pages within this application. As well as showcase what is happening on the backend ~ Model + SQL (docker).
    </p>
</div>

<div class="detailTable">
    <div class="container text-center" style="font-size: 25px; margin-top: 25px; margin-bottom: 25px">
        <div>
            <div class="row">
                <div class="col">
                    <a asp-area="" asp-controller="Coffee" asp-action="Details"> Drinks </a>
                </div>
                <div class="col">
                    <a asp-area="" asp-controller="Coffee" asp-action="Create"> Create </a>
                </div>
            </div>

            <div class="row">
                <div class="col">
                    <a asp-area="" asp-controller="Coffee" asp-action="Edit"> Edit </a>
                </div>
                <div class="col">
                    <a asp-area="" asp-controller="Coffee" asp-action="Delete"> Delete </a>
                </div>
            </div>
        </div>
    </div>
</div>
